{% extends '@EasyAdmin/crud/form_theme.html.twig' %}
{% block field %}
    {% if field.type == 'association' %}
        {% if field.type_options.mapped %}
            {{ form_widget(form[field.property], { 'attr': {'class': 'form-control'} }) }}
        {% else %}
            {{ form_widget(form[field.property], { 'attr': {'class': 'form-control', 'multiple': 'multiple'} }) }}
        {% endif %}
    {% else %}
        {{ parent() }}
    {% endif %}
{% endblock %}
